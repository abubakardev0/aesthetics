{
    "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
    "globals": {
        "React": "readonly"
    },
    "rules": {
        "no-unused-vars": [
            1,
            { "args": "after-used", "argsIgnorePattern": "^_" }
        ],
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": "off"
    }
}
